{
  "name": "tntu-module",
  "version": "0.0.1",
  "main": "index.js",
  "scripts": {
    "commit": "git-cz",
    "setup": "./node_modules/.bin/node-lambda setup",
    "test": "./node_modules/.bin/node-lambda run",
    "package": "./node_modules/.bin/node-lambda package",
    "deploy": "rm function.zip; npm run build && zip -r function.zip -q . && aws lambda update-function-code --function-name tntu-module --zip-file fileb://function.zip",
    "zip": "rm function.zip; npm run build && zip -r function.zip -q .",
    "deploy__": "./node_modules/.bin/node-lambda deploy",
    "build": "babel src --out-dir dist",
    "start": "npm run build && npm run test",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "release": "standard-version",
    "release:major": "npm run release -- --release-as major",
    "release:minor": "npm run release -- --release-as minor",
    "release:patch": "npm run release -- --release-as patch",
    "prod-modules": "rm -rf ./node_modules; npm i --only=prod"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.3",
    "@babel/core": "^7.8.3",
    "@babel/node": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "aws-xray-sdk": "^2.5.0",
    "babel-eslint": "^10.1.0",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-lambdas": "^0.2.5",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.0",
    "lint-staged": "^10.0.2",
    "node-lambda": "^0.9.0",
    "prettier": "^1.19.1",
    "standard-version": "^7.1.0"
  },
  "dependencies": {
    "aws-sdk": "^2.814.0",
    "jsdom": "^16.2.0",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "serialize-error": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./src/*.js": [
      "eslint --fix"
    ]
  }
}
